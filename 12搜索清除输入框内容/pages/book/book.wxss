.search{
    width:100%;
    height:100rpx;
    padding-left:20rpx;
    padding-right:20rpx;
    background: #fff;
    border-top: 1rpx solid #F7F7F7;
    position: fixed;
    left:0;
    top:0;
    z-index: 200;
}
.search-input{
    width:690rpx;
    height:70rpx;
    background: #F7F7F7;
    border-radius: 30rpx;

}
page{
    height:100%;
    background: #F9F9F9;
}
.search-input text{
    font-size: 30rpx;
    color:#A6A6A6;
}
.select{
    margin-top: 150rpx;
    font-size: 30rpx;
}
.container{
    margin-top: 50rpx;
    padding-left:90rpx;
    padding-right:90rpx;
    display: flex;
    flex-wrap: wrap;
    justify-content: space-between;
}
v-book{
    margin-bottom: 60rpx;
}