.header{
    height:100rpx;
    display: flex;
    padding-left:20rpx;
    padding-right:20rpx;
    align-items: center;
    border-top: 1rpx solid #eee;
    border-bottom: 1rpx solid #eee;
    flex-direction: row;
}
.search{
    width:530rpx;
    height:70rpx;
    background: rgb(245, 245, 245);
    border-radius:30rpx;
    padding-left: 20rpx;
    display: flex;
    align-items: center;
}
.search input{
    flex:1;
    margin-left: 20rpx;
}
.cancel{
    height:70rpx;
    border-radius: 30rpx;
    line-height: 70rpx;
    border-color: #888;
}
.container{
    padding: 20rpx;
}
.title{
    display: flex;
    height:90rpx;
    align-items: center;
}
.line{
    height:30rpx;
    width:10rpx;
    background: #333;
}