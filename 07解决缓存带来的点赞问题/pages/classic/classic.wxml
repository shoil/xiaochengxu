<view class="header">
    <v-date index="{{classic.index}}"></v-date>
    <v-like like="{{like}}" count="{{count}}" bind:like="onLike"></v-like>
</view>
<v-movie image="{{classic.image}}" content="{{classic.content}}"></v-movie>
<v-nav title="{{classic.title}}" isLatest="{{isLatest}}" isFirst="{{isFirst}}"
bind:previous="onPrev" bind:next="onNext"
></v-nav>